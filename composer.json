{
    "name": "programic/laravel-http-logger",
    "description": "A Laravel package to log HTTP requests",
    "keywords": [
        "programic",
        "laravel-http-logger"
    ],
    "homepage": "https://github.com/programic/laravel-http-logger",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.22|^2.34",
        "phpunit/phpunit": "^9.0|^10.5"
    },
    "autoload": {
        "psr-4": {
            "Programic\\HttpLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programic\\HttpLogger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\HttpLogger\\HttpLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
